<?xml version="1.0" encoding="UTF-8" standalone="yes" ?>
<CodeBlocks_project_file>
	<FileVersion major="1" minor="6" />
	<Project>
		<Option title="jv-core" />
		<Option makefile_is_custom="1" />
		<Option pch_mode="2" />
		<Option compiler="mingw" />
		<Build>
			<Target title="Debug">
				<Option output="bin/Debug/jv-core" prefix_auto="1" extension_auto="1" />
				<Option object_output="obj/Debug/" />
				<Option type="1" />
				<Option compiler="mingw" />
				<Compiler>
					<Add option="-g" />
					<Add option="-finput-charset=UTF-8" />
					<Add option="-fexec-charset=UTF-8" />
					<Add directory="." />
				</Compiler>
				<MakeCommands>
					<Build command="$make -f $makefile" />
					<CompileFile command="$make -f $makefile" />
					<Clean command="$make -f $makefile clean" />
					<DistClean command="$make -f $makefile distclean$target" />
					<AskRebuildNeeded command="$make -q -f $makefile" />
					<SilentBuild command="$make -f $makefile &gt; $(CMD_NULL)" />
				</MakeCommands>
			</Target>
			<Target title="Release">
				<Option output="bin/Release/jv-core" prefix_auto="1" extension_auto="1" />
				<Option object_output="obj/Release/" />
				<Option type="1" />
				<Option compiler="mingw" />
				<Compiler>
					<Add option="-O2" />
					<Add option="-finput-charset=UTF-8" />
					<Add option="-fexec-charset=UTF-8" />
					<Add directory="." />
				</Compiler>
				<Linker>
					<Add option="-s" />
				</Linker>
			</Target>
		</Build>
		<Compiler>
			<Add option="-Wall" />
		</Compiler>
		<Unit filename="jv_config.h" />
		<Unit filename="jv_core.h" />
		<Unit filename="jv_log.c">
			<Option compilerVar="CC" />
		</Unit>
		<Unit filename="jv_log.h" />
		<Unit filename="jv_main.c">
			<Option compilerVar="CC" />
		</Unit>
		<Unit filename="jv_pool.c">
			<Option compilerVar="CC" />
		</Unit>
		<Unit filename="jv_pool.h" />
		<Unit filename="jv_string.c">
			<Option compilerVar="CC" />
		</Unit>
		<Unit filename="jv_string.h" />
		<Unit filename="jv_time.c">
			<Option compilerVar="CC" />
		</Unit>
		<Unit filename="jv_time.h" />
		<Extensions>
			<code_completion />
			<envvars />
			<debugger />
		</Extensions>
	</Project>
</CodeBlocks_project_file>
